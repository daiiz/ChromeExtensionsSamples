   "icons": {
      "128": "e128.png",
      "48": "e128.png",
      "16": "e128.png"
   },
   "manifest_version": 2,
   "name": "ChromeExtensionsBase",
   "permissions": ["tabs"],
   "version": "0.0.1"
}
